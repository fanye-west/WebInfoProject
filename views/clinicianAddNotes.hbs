<div class="main_page_body_under_navigation">
        <div class="light_theme_colour_background four_corners" id="note_page">
            <h1 id="patient_note_heading">Patient Notes</h1>

            <div class="add_note_form">
                <h3>Add new note:</h3>

                <form class="add_note_form"method="post">
                    <textarea class="data_comment white" name="add_note" id="add_note" cols="30" rows="10"></textarea>
                    <div class="data_entry_button_grid add_note_buttons">
                        <div class="outer_block_text_column" id="cancel_button">
                            <button class="default_button grey" id="data_button" type="button"
                                onclick="patientRedirectHomeNoAction()">Cancel</button>
                        </div>

                        <div class="outer_block_text_column" id="submit_button">
                            <input type="submit" value="Submit" class="default_button dark_blue" id="data_button">
                        </div>
                    </div>

            </form>



        </div>

